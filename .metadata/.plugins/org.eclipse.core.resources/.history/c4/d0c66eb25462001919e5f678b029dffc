package com.faxing;

public class MyNumber/* <T extends Integer, E extends Float> */ {
//	private T tValue;
//	private E eValue;
	private Number number;

	public <T extends Integer> void setNumber(T t) {
		this.number = t;
	}

	public <T extends Float> void setNumber(T t) {
		this.number = t;
	}

	public Number getNumber() {
		return number;
	}

	public static void main(String[] args) {
//		MyNumber<Integer, Float> number = new MyNumber<Integer, Float>();
		MyNumber number = new MyNumber();
		number.setNumber(3.3f);

		System.out.println(number.getNumber());
	}
}
