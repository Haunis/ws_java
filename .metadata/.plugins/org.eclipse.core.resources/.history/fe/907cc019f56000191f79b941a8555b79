package com.fanxing

/*
 泛型方法
 */

fun <T> getBox(value: T) = Box(value)
fun <T> doPrinln(value: T) {
	when (value) {
		is Int -> println("value is Int : ${value}")
		is String -> println("value is Int : ${value}")
		is Boolean -> println("value is Int : ${value}")
	}
}

fun main(args: Array<String>) {
	val intBox = getBox(3)
	println("intBox : ${intBox.value}");

	val age: Int = 24
	val name: String = "zhang san"
	val isMale: Boolean = false
}
