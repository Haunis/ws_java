package com.jiage.spinlock;

public class MainTest {
	public static void main(String[] args) {
		SpinLock sl = new SpinLock();

		for (int i = 0; i < 10; i++) {
			new Thread(() -> {
				sl.lock();
				try {
					Thread.sleep(1000);
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
				System.out.println(Thread.currentThread().getName() + " run");
				sl.unLock();
			}).start();
		}

	}
}
