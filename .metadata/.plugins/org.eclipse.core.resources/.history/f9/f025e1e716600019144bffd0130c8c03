package com.data_sealed


sealed class Expre
class Const(val num: Double) : Expre() {}
class Sum(val e1: Expre, val e2: Expre) : Expre() {}
object NotNum : Expre() {}

fun eval(ex: Expre): Double = when (ex) {
	is Expre.Const -> ex.num
	is Expre.Sum -> eval(ex.e1) + eval(ex.e2)
	Expre.NotNum -> Double.NaN
}