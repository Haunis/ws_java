package com.aaa.test;

public class AAAmain {
	public static void main(String[] args) {
		final int count = 0;
		Thread t1 = new Thread(new Runnable() {
			@Override
			public void run() {
				for (int i = 0; i < 500; i++) {
					count++;
				}
			};
		});
		Thread t2 = new Thread(new Runnable() {
			@Override
			public void run() {
				for (int i = 0; i < 500; i++) {
					count++;
				}
			};
		});

		t1.start();
		t2.start();
		System.out.println(count);
	}
}
