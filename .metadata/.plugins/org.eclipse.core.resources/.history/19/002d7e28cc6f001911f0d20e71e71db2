package com.expression

/*
 表达式
 lambda表达式可以定义一个函数
 
 */

//lamabda可以作为一种类型，被定义成val;调用这个lambda类型与调用函数printMsg2(ms:String) 无异
val printMsg = { msg: String ->
	println(msg)
}

fun printMsg2(msg: String) {
	println(msg)
}

fun main(args: Array<String>) {
	//lambda类型的调用
	printMsg("this is lambda ")
	printMsg2("this is normal")
}



