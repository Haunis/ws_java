package com.enum_demo

enum class Color {
	RED,
	BLUE,
	GREEN

}

fun main(args: Array<String>) {
	val color = Color.RED
	val colors: Array<Color> = Color.values()

	println("colors: ${colors}")
	println(Color.valueOf("RED"))
	println("color.name = ${color.name}")
	println("color.ordinal = ${color.ordinal}")
}

